{
  "compilerOptions": {
    "lib": ["ESNext"],
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "typeRoots": ["node_modules/@types"],

    // so that if your project isn't using TypeScript, it still has autocomplete
    "allowJs": true,

    // "bun-types" is the important part
    "types": ["bun-types","cheerio", "express"]
  }
}
